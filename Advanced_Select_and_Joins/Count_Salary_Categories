select k.category , k.counti as accounts_count
 from
(
select 'Low Salary' as category , count(income) as counti
from Accounts
where income<20000
union all
select 'Average Salary' as category , count(income) as counti
from Accounts
where income  between 20000 and 50000
union all
select 'High Salary' as category ,count(income) as counti
from Accounts
where income>50000
) k
order by k.counti desc
