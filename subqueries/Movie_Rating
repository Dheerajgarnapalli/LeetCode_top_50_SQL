select t1.name as results
from(
select u.name , count(q.rating) as ratingu
from Users u 
inner join 
Movies m
left join
MovieRating q
on u.user_id = q.user_id and m.movie_id = q.movie_id
group by u.name
order by ratingu desc ,u.name asc
limit 1
 ) t1
union all
select t2.title as results
from (
select m.title , avg(rating) as average
from MovieRating q
left join 
Movies m
on q.movie_id = m.movie_id
where month(q.created_at) =2 and year(q.created_at)=2020
group by m.title
order by average desc , m.title asc
limit 1
) t2
